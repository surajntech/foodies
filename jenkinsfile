pipeline{
    agent any
    tools {
        Maven "maven"
    }

    stages{
        stage('clean workspace'){
            steps {
                cleanWs()
            }
        }
        stage('git checkout') {
            steps {
                git credentialsId: 'git-creds', url: 'https://github.com/surajntech/foodies.git'
            }
        }
        stage('maven test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}